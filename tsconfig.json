{
  "compilerOptions": {
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "strictNullChecks": true,
      "target": "ES5",
      "moduleResolution": "Node",
      "module": "CommonJS",
      "declaration": true,
      "isolatedModules": true,
      "noEmit": true,
      "outDir": "dist",
      "jsx": "react-jsx"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}